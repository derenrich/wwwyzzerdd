{
    "manifest_version": 2,
    "name": "Wwwyzzerdd for Wikidata",
    "version": "0.0.2.2",
    "description": "Edit wikidata from wikipedia!",
    "icons": { },
    "page_action": { },
    "commands": { },
    "content_scripts": [{
        "matches": ["*://*.wikipedia.org/*"],
        "js": ["frontend.tsx"],
        "css": [],
        "run_at": "document_idle"
    }],
    "event_rules": [],
    "externally_connectable": {
        "matches": []
    },
    "file_browser_handlers": [ ],
    "file_system_provider_capabilities": {
        "configurable": false,
        "multiple_mounts": false,
        "source": "file"
    },
    "homepage_url": "https://github.com/derenrich/wwwyzzerdd",
    "offline_enabled": false,
    "permissions": ["storage",
                    "alarms",
                    "activeTab",
                    "contextMenus",
                    "unlimitedStorage",
                    "https://www.wikidata.org/*",
                    "https://*.wikipedia.org/*"                  
                   ],
    "requirements": {},
    "sandbox": [],
    "short_name": "wwwyzzerdd",
    "storage": {},
    "web_accessible_resources": [],
    "background": {
        "scripts": ["background.ts"]
    }    
}
